    "import sqlite3\n",
    "import pandas as pd\n",
    "\n",
    "conn = sqlite3.connect(\"../internship.db\")\n",
    "\n",
    "query = \"\"\"\n",
    "SELECT \n",
    "    interns.name,\n",
    "    interns.track,\n",
    "    mentors.mentor_name\n",
    "FROM interns\n",
    "INNER JOIN mentors\n",
    "ON interns.track = mentors.track;\n",
    "\"\"\"\n",
    "\n",
    "df = pd.read_sql_query(query, conn)\n",
    "\n",
    "df"
   ]
